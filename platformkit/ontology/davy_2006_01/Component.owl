<?xml version='1.0' encoding='ISO-8859-1'?>
<!DOCTYPE uridef[
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema">
  <!ENTITY owl "http://www.w3.org/2002/07/owl">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema">
  <!ENTITY platform "http://www.cs.kuleuven.be/~distrinet/projects/CoDAMoS/2006/01/Platform.owl">
  <!ENTITY DEFAULT "http://www.cs.kuleuven.be/~davy/owl/2006/01/Component.owl">
]>


<rdf:RDF
  xmlns:rdf = "&rdf;#"
  xmlns:rdfs = "&rdfs;#"
  xmlns:owl = "&owl;#"
  xmlns:xsd = "&xsd;#"
  xmlns:platform = "&platform;#"
  xml:base = "&DEFAULT;"
  xmlns = "&DEFAULT;#">

  <owl:Ontology rdf:about="">
    <owl:versionInfo>
      $Id: Component.owl 292 2006-09-25 18:19:37Z dennis $
    </owl:versionInfo>
    <rdfs:comment>
      OWL ontology for components
      Created by:
        Davy Preuveneers
    </rdfs:comment>
    <owl:imports rdf:resource="&platform;" />
  </owl:Ontology>


<!--   ############ ########### ############ ############ ########### -->
<!--   ############                                       ########### -->
<!--   ############        COMPONENT SPECIFICATION        ########### -->
<!--   ############                                       ########### -->
<!--   ############ ########### ############ ############ ########### -->

  <owl:Class rdf:ID="ComponentBlueprint" />

  <owl:DatatypeProperty rdf:about="#hasImplementation">
    <rdfs:domain rdf:resource="#ComponentBlueprint"/>
    <rdfs:range rdf:resource="&xsd;#anyURI"/>
  </owl:DatatypeProperty>

  <owl:Class rdf:ID="PortBlueprint" />
  
  <owl:Class rdf:ID="MulticastPort">
    <rdfs:subClassOf rdf:resource="#PortBlueprint" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="OutMessage" />
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasMessage" />
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:ID="PortGroup">
    <rdfs:subClassOf rdf:resource="#PortBlueprint" />
  </owl:Class>

  <owl:ObjectProperty rdf:about="#hasPort">
    <rdfs:domain rdf:resource="#ComponentBlueprint" />
    <rdfs:range rdf:resource="#PortBlueprint" />
  </owl:ObjectProperty>

  <owl:DatatypeProperty rdf:about="#maxInstances">
    <rdfs:domain rdf:resource="#PortBlueprint" />
    <rdfs:range rdf:resource="&xsd;#int" />
    <rdf:type rdf:resource="&owl;#FunctionalProperty" />
  </owl:DatatypeProperty>

  <owl:ObjectProperty rdf:about="#hasMessage">
    <rdfs:domain rdf:resource="#PortBlueprint" />
    <rdfs:range rdf:resource="#Message" />
  </owl:ObjectProperty>


<!--   ############ ########### ############ ############ ########### -->
<!--   ############                                       ########### -->
<!--   ############         MESSAGE SPECIFICATION         ########### -->
<!--   ############                                       ########### -->
<!--   ############ ########### ############ ############ ########### -->

  <owl:Class rdf:ID="Message" />

  <owl:Class rdf:about="#OutMessage">
    <rdfs:subClassOf rdf:resource="#Message" />
  </owl:Class>
  
  <owl:Class rdf:ID="InMessage">
    <rdfs:subClassOf rdf:resource="#Message" />
  </owl:Class>

  <owl:Class rdf:ID="Parameter" />

  <owl:ObjectProperty rdf:about="#hasParameter">
    <rdfs:domain rdf:resource="#Message" />
    <rdfs:range rdf:resource="#Parameter" />
  </owl:ObjectProperty>

  <owl:DatatypeProperty rdf:about="#parameterType">
    <rdfs:domain rdf:resource="#Parameter" />
    <rdf:type rdf:resource="&owl;#FunctionalProperty" />
  </owl:DatatypeProperty>


<!--   ############ ########### ############ ############ ########### -->
<!--   ############                                       ########### -->
<!--   ############   COMPOSITE  COMPONENT SPECIFICATION  ########### -->
<!--   ############                                       ########### -->
<!--   ############ ########### ############ ############ ########### -->

  <owl:Class rdf:ID="ComponentComposition" />

  <owl:Class rdf:ID="CompositeComponent">
    <rdfs:subClassOf rdf:resource="#ComponentBlueprint" />
    <rdfs:subClassOf rdf:resource="#ComponentComposition" />
  </owl:Class>
  
  <owl:ObjectProperty rdf:about="#hasComponentInstance">
    <rdfs:domain rdf:resource="#ComponentComposition" />
    <rdfs:range rdf:resource="#ComponentInstance" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasConnector">
    <rdfs:domain rdf:resource="#ComponentComposition" />
    <rdfs:range rdf:resource="#Connector" />
  </owl:ObjectProperty>

  <owl:Class rdf:ID="ComponentInstance" />

  <owl:DatatypeProperty rdf:about="#hasName">
    <rdfs:domain rdf:resource="#ComponentInstance"/>
    <rdfs:range rdf:resource="&xsd;#string"/>
  </owl:DatatypeProperty>

  <owl:ObjectProperty rdf:about="#hasAlternative">
    <rdfs:domain rdf:resource="#ComponentInstance"/>
    <rdfs:range rdf:resource="#ComponentInstance"/>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:about="#isOptional">
    <rdfs:domain rdf:resource="#ComponentInstance"/>
    <rdfs:range rdf:resource="#Connector"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasComponentBlueprint">
    <rdfs:domain rdf:resource="#ComponentInstance"/>
    <rdfs:range rdf:resource="#ComponentBlueprint"/>
    <rdf:type rdf:resource="&owl;#FunctionalProperty" />
  </owl:ObjectProperty>
 
  <owl:ObjectProperty rdf:about="#hasPortInstance">
    <rdfs:domain rdf:resource="#ComponentInstance" />
    <rdfs:range rdf:resource="#PortInstance" />
  </owl:ObjectProperty>

  <owl:Class rdf:ID="PortInstance" />

  <owl:ObjectProperty rdf:about="#hasPortBlueprint">
    <rdfs:domain rdf:resource="#PortInstance"/>
    <rdfs:domain rdf:resource="#PortBlueprint"/>
    <rdf:type rdf:resource="&owl;#FunctionalProperty" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#exportAs">
    <rdfs:domain rdf:resource="#PortInstance" />
    <rdfs:range rdf:resource="#PortBlueprint" />
  </owl:ObjectProperty>


<!--   ############ ########### ############ ############ ########### -->
<!--   ############                                       ########### -->
<!--   ############        CONNECTOR SPECIFICATION        ########### -->
<!--   ############                                       ########### -->
<!--   ############ ########### ############ ############ ########### -->

  <owl:Class rdf:ID="Connector" />

  <owl:ObjectProperty rdf:about="#to">
    <rdfs:domain rdf:resource="#Connector" />
    <rdfs:range rdf:resource="#PortInstance" />
    <rdf:type rdf:resource="&owl;#FunctionalProperty" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#from">
    <rdfs:domain rdf:resource="#Connector" />
    <rdfs:range rdf:resource="#PortInstance" />
    <rdf:type rdf:resource="&owl;#FunctionalProperty" />
  </owl:ObjectProperty>


<!--   ############ ########### ############ ############ ########### -->
<!--   ############                                       ########### -->
<!--   ############         CONTRACT SPECIFICATION        ########### -->
<!--   ############                                       ########### -->
<!--   ############ ########### ############ ############ ########### -->

  <owl:Class rdf:ID="Contract" />

  <owl:Class rdf:ID="Comparator">
    <owl:oneOf rdf:parseType="Collection">
      <Comparator rdf:ID="eq" />
      <Comparator rdf:ID="lt" />
      <Comparator rdf:ID="gt" />
      <Comparator rdf:ID="le" />
      <Comparator rdf:ID="ge" />
     </owl:oneOf>
  </owl:Class>
  
  <owl:ObjectProperty rdf:about="#contractResource">
    <rdfs:domain rdf:resource="#Contract" />
    <rdfs:range rdf:resource="&platform;#Resource" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#contractCondition">
    <rdfs:domain rdf:resource="#Contract" />
    <rdfs:range rdf:resource="#Comparator" />
  </owl:ObjectProperty>

  <owl:DatatypeProperty rdf:about="#contractValue">
    <rdfs:domain rdf:resource="#Contract" />
    <rdfs:range rdf:resource="&xsd;#int" />
  </owl:DatatypeProperty>

</rdf:RDF>

