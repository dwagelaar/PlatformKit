<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE uridef[
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema">
  <!ENTITY owl "http://www.w3.org/2002/07/owl">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema">
  <!ENTITY units "http://www.cs.kuleuven.be/~distrinet/projects/CoDAMoS/2005/01/Units.owl">
  <!ENTITY platform "http://www.cs.kuleuven.be/~distrinet/projects/CoDAMoS/2007/01/Platform.owl">
  <!ENTITY DEFAULT "http://ssel.vub.ac.be/codamos/2007/01/PackageManagers.owl">
]>

<rdf:RDF
  xmlns:rdf = "&rdf;#"
  xmlns:rdfs = "&rdfs;#"
  xmlns:owl = "&owl;#"
  xmlns:xsd = "&xsd;#"
  xmlns:units = "&units;#"
  xmlns:platform = "&platform;#"
  xmlns = "&DEFAULT;#"
  xml:base = "&DEFAULT;">

  <owl:Ontology rdf:about="">
    <rdfs:label xml:lang="en">CoDAMoS package managers ontology</rdfs:label>
    <owl:versionInfo rdf:datatype="&xsd;#string">$Id$</owl:versionInfo>
    <rdfs:comment xml:lang="en">Package manager concepts. Created by Dennis Wagelaar.</rdfs:comment>
    <owl:imports rdf:resource="&platform;"/>
  </owl:Ontology>

  <!-- ====================================================================== -->
  <!-- Package managers                                                       -->
  <!-- ====================================================================== -->

  <owl:Class rdf:ID="CAB">
    <rdfs:subClassOf rdf:resource="&platform;#PackageManager"/>
    <owl:disjointWith rdf:resource="#DMG"/>
    <owl:disjointWith rdf:resource="#Dpkg"/>
    <owl:disjointWith rdf:resource="#Ipkg"/>
    <owl:disjointWith rdf:resource="#MSI"/>
    <owl:disjointWith rdf:resource="#Pkgtool"/>
    <owl:disjointWith rdf:resource="#RPM"/>
  </owl:Class>

  <owl:Class rdf:ID="DMG">
    <rdfs:subClassOf rdf:resource="&platform;#PackageManager"/>
    <owl:disjointWith rdf:resource="#CAB"/>
    <owl:disjointWith rdf:resource="#Dpkg"/>
    <owl:disjointWith rdf:resource="#Ipkg"/>
    <owl:disjointWith rdf:resource="#MSI"/>
    <owl:disjointWith rdf:resource="#Pkgtool"/>
    <owl:disjointWith rdf:resource="#RPM"/>
  </owl:Class>

  <owl:Class rdf:ID="Dpkg">
    <rdfs:subClassOf rdf:resource="&platform;#PackageManager"/>
    <owl:disjointWith rdf:resource="#DMG"/>
    <owl:disjointWith rdf:resource="#CAB"/>
    <owl:disjointWith rdf:resource="#Ipkg"/>
    <owl:disjointWith rdf:resource="#MSI"/>
    <owl:disjointWith rdf:resource="#Pkgtool"/>
    <owl:disjointWith rdf:resource="#RPM"/>
  </owl:Class>
  
  <owl:ObjectProperty rdf:ID="hasDpkgVersion">
    <rdfs:domain rdf:resource="#Dpkg"/>
    <rdfs:range rdf:resource="&platform;#Version"/>
    <rdf:type rdf:resource="&owl;#FunctionalProperty"/>
  </owl:ObjectProperty>

  <owl:Class rdf:ID="Fink">
    <rdfs:subClassOf rdf:resource="#Dpkg"/>
  </owl:Class>

  <owl:Class rdf:ID="Ipkg">
    <rdfs:subClassOf rdf:resource="&platform;#PackageManager"/>
    <owl:disjointWith rdf:resource="#DMG"/>
    <owl:disjointWith rdf:resource="#Dpkg"/>
    <owl:disjointWith rdf:resource="#CAB"/>
    <owl:disjointWith rdf:resource="#MSI"/>
    <owl:disjointWith rdf:resource="#Pkgtool"/>
    <owl:disjointWith rdf:resource="#RPM"/>
  </owl:Class>
  
  <owl:ObjectProperty rdf:ID="hasIpkgVersion">
    <rdfs:domain rdf:resource="#Ipkg"/>
    <rdfs:range rdf:resource="&platform;#Version"/>
    <rdf:type rdf:resource="&owl;#FunctionalProperty"/>
  </owl:ObjectProperty>

  <owl:Class rdf:ID="MSI">
    <rdfs:subClassOf rdf:resource="&platform;#PackageManager"/>
    <owl:disjointWith rdf:resource="#DMG"/>
    <owl:disjointWith rdf:resource="#Dpkg"/>
    <owl:disjointWith rdf:resource="#Ipkg"/>
    <owl:disjointWith rdf:resource="#CAB"/>
    <owl:disjointWith rdf:resource="#Pkgtool"/>
    <owl:disjointWith rdf:resource="#RPM"/>
  </owl:Class>
  
  <owl:ObjectProperty rdf:ID="hasMSIVersion">
    <rdfs:domain rdf:resource="#MSI"/>
    <rdfs:range rdf:resource="&platform;#Version"/>
    <rdf:type rdf:resource="&owl;#FunctionalProperty"/>
  </owl:ObjectProperty>

  <owl:Class rdf:ID="Pkgtool">
    <rdfs:subClassOf rdf:resource="&platform;#PackageManager"/>
    <owl:disjointWith rdf:resource="#DMG"/>
    <owl:disjointWith rdf:resource="#Dpkg"/>
    <owl:disjointWith rdf:resource="#Ipkg"/>
    <owl:disjointWith rdf:resource="#MSI"/>
    <owl:disjointWith rdf:resource="#CAB"/>
    <owl:disjointWith rdf:resource="#RPM"/>
  </owl:Class>
  
  <owl:ObjectProperty rdf:ID="hasPkgToolVersion">
    <rdfs:domain rdf:resource="#Pkgtool"/>
    <rdfs:range rdf:resource="&platform;#Version"/>
    <rdf:type rdf:resource="&owl;#FunctionalProperty"/>
  </owl:ObjectProperty>

  <owl:Class rdf:ID="RPM">
    <rdfs:subClassOf rdf:resource="&platform;#PackageManager"/>
    <owl:disjointWith rdf:resource="#DMG"/>
    <owl:disjointWith rdf:resource="#Dpkg"/>
    <owl:disjointWith rdf:resource="#Ipkg"/>
    <owl:disjointWith rdf:resource="#MSI"/>
    <owl:disjointWith rdf:resource="#Pkgtool"/>
    <owl:disjointWith rdf:resource="#CAB"/>
  </owl:Class>

  <owl:ObjectProperty rdf:ID="hasRPMVersion">
    <rdfs:domain rdf:resource="#RPM"/>
    <rdfs:range rdf:resource="&platform;#Version"/>
    <rdf:type rdf:resource="&owl;#FunctionalProperty"/>
  </owl:ObjectProperty>

</rdf:RDF>
