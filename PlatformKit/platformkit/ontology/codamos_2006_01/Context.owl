<?xml version='1.0' encoding='ISO-8859-1'?>
<!DOCTYPE uridef[
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema">
  <!ENTITY owl "http://www.w3.org/2002/07/owl">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema">
  <!ENTITY platform "http://www.cs.kuleuven.be/~distrinet/projects/CoDAMoS/2006/01/Platform.owl">
  <!ENTITY service "http://www.cs.kuleuven.be/~distrinet/projects/CoDAMoS/2006/01/Service.owl">
  <!ENTITY env "http://www.cs.kuleuven.be/~distrinet/projects/CoDAMoS/2006/01/Environment.owl">
  <!ENTITY user "http://www.cs.kuleuven.be/~distrinet/projects/CoDAMoS/2006/01/User.owl">
  <!ENTITY DEFAULT "http://www.cs.kuleuven.be/~distrinet/projects/CoDAMoS/2006/01/Context.owl">
]>

<rdf:RDF
  xmlns:rdf = "&rdf;#"
  xmlns:rdfs = "&rdfs;#"
  xmlns:owl = "&owl;#"
  xmlns:xsd = "&xsd;#"
  xmlns:platform = "&platform;#"
  xmlns:service = "&service;#"
  xmlns:env = "&env;#"
  xmlns:user = "&user;#"
  xmlns = "&DEFAULT;#"
  xml:base = "&DEFAULT;">
 
  <owl:Ontology rdf:about="">
    <rdfs:label xml:lang="en">CoDAMoS context ontology</rdfs:label>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
      $Id: Context.owl 242 2006-02-09 07:49:14Z dennis $
    </owl:versionInfo> 
    <rdfs:comment>
      Context ontology concepts and definitions
      Created by:
        Davy Preuveneers
    </rdfs:comment>
    <owl:imports rdf:resource="&platform;" />
    <owl:imports rdf:resource="&service;" />
    <owl:imports rdf:resource="&env;" />
    <owl:imports rdf:resource="&user;" />
  </owl:Ontology>

  

<!--   ############ ########### ############ ############ ########### -->
<!--   ############                                       ########### -->
<!--   ############          PLATFORM PARAMETERS          ########### -->
<!--   ############                                       ########### -->
<!--   ############ ########### ############ ############ ########### -->

  <owl:ObjectProperty rdf:ID="providesService">
    <rdfs:range rdf:resource="&service;#Service" />
    <rdfs:domain rdf:resource="&platform;#Software" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="hasEnvironment">
    <rdf:type rdf:resource="&owl;#FunctionalProperty" />
    <rdfs:domain rdf:resource="&platform;#Platform" />
    <rdfs:range rdf:resource="&env;#Environment" />
  </owl:ObjectProperty>



<!--   ############ ########### ############ ############ ########### -->
<!--   ############                                       ########### -->
<!--   ############            USER PARAMETERS            ########### -->
<!--   ############                                       ########### -->
<!--   ############ ########### ############ ############ ########### -->

  <owl:ObjectProperty rdf:ID="usesService">
    <rdfs:range rdf:resource="&service;#Service" />
    <rdfs:domain rdf:resource="&user;#Task" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="usesIODevice">
    <rdfs:domain rdf:resource="&user;#User" />
    <rdfs:range rdf:resource="&platform;#IODevice" />
  </owl:ObjectProperty>
  
</rdf:RDF>
