<?xml version='1.0' encoding='ISO-8859-1'?>
<!DOCTYPE uridef[
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema">
  <!ENTITY owl "http://www.w3.org/2002/07/owl">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema">
  <!ENTITY units "http://www.cs.kuleuven.be/~distrinet/projects/CoDAMoS/2005/01/Units.owl">
  <!ENTITY DEFAULT "http://www.cs.kuleuven.be/~distrinet/projects/CoDAMoS/2005/01/Context.owl">
]>

<rdf:RDF
  xmlns:rdf = "&rdf;#"
  xmlns:rdfs = "&rdfs;#"
  xmlns:owl = "&owl;#"
  xmlns:xsd = "&xsd;#"
  xmlns:units = "&units;#"
  xmlns = "&DEFAULT;#"
  xml:base = "&DEFAULT;">
 
  <owl:Ontology rdf:about="">
    <rdfs:label xml:lang="en">CoDAMoS context ontology</rdfs:label>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
      $Id: Context.owl,v 1.0 2005/04/10 14:23 davy Exp $
    </owl:versionInfo> 
    <rdfs:comment>
      Context ontology concepts and definitions
      Created by:
        Davy Preuveneers
    </rdfs:comment>
    <owl:imports rdf:resource="&units;" />
  </owl:Ontology>

  

<!--   ############ ########### ############ ############ ########### -->
<!--   ############                                       ########### -->
<!--   ############          SERVICE PARAMETERS           ########### -->
<!--   ############                                       ########### -->
<!--   ############ ########### ############ ############ ########### -->

  <owl:Class rdf:ID="Service" />

  <owl:Class rdf:ID="ServiceProfile" />

  <owl:Class rdf:ID="ServiceModel" />

  <owl:Class rdf:ID="ServiceGrounding" />

  <owl:ObjectProperty rdf:ID="hasServiceModel">
    <rdfs:range rdf:resource="#ServiceModel" />
    <rdf:type rdf:resource="&owl;#FunctionalProperty" />
    <rdfs:domain rdf:resource="#Service" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="hasServiceGrounding">
    <rdfs:domain rdf:resource="#Service" />
    <rdfs:range rdf:resource="#ServiceGrounding" />
    <rdf:type rdf:resource="&owl;#FunctionalProperty" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="hasServiceProfile">
    <rdfs:range rdf:resource="#ServiceProfile" />
    <rdfs:domain rdf:resource="#Service" />
    <rdf:type rdf:resource="&owl;#FunctionalProperty" />
  </owl:ObjectProperty>



<!--   ############ ########### ############ ############ ########### -->
<!--   ############                                       ########### -->
<!--   ############          RESOURCE PARAMETERS          ########### -->
<!--   ############                                       ########### -->
<!--   ############ ########### ############ ############ ########### -->

  <owl:Class rdf:ID="Resource">
    <rdfs:subClassOf rdf:resource="#Hardware" />
  </owl:Class>

  <owl:ObjectProperty rdf:ID="maxCapacity">
    <rdf:type rdf:resource="&owl;#FunctionalProperty" />
    <rdfs:domain rdf:resource="#Resource" />
    <rdfs:range rdf:resource="&units;#Value" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="currAvailable">
    <rdf:type rdf:resource="&owl;#FunctionalProperty" />
    <rdfs:domain rdf:resource="#Resource" />
    <rdfs:range rdf:resource="&units;#Value" />
  </owl:ObjectProperty>

  <owl:Class rdf:ID="PowerResource">
    <rdfs:subClassOf rdf:resource="#Resource" />
  </owl:Class>

  <owl:Class rdf:ID="MemoryResource">
    <rdfs:subClassOf rdf:resource="#Resource" />
  </owl:Class>

  <owl:Class rdf:ID="NetworkResource">
    <rdfs:subClassOf rdf:resource="#Resource" />
  </owl:Class>

  <owl:ObjectProperty rdf:ID="reliability">
    <rdf:type rdf:resource="&owl;#FunctionalProperty" />
    <rdfs:domain rdf:resource="#NetworkResource" />
    <rdfs:range rdf:resource="&units;#IntegerValue" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="latency">
    <rdf:type rdf:resource="&owl;#FunctionalProperty" />
    <rdfs:domain rdf:resource="#NetworkResource" />
    <rdfs:range rdf:resource="&units;#IntegerValue" />
  </owl:ObjectProperty>

  <owl:Class rdf:ID="CPUResource">
    <rdfs:subClassOf rdf:resource="#Resource" />
  </owl:Class>

  <owl:ObjectProperty rdf:ID="instructionSet">
    <rdf:type rdf:resource="&owl;#FunctionalProperty" />
    <rdfs:domain rdf:resource="#CPUResource" />
    <rdfs:range rdf:resource="&units;#StringValue" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="cpuCache">
    <rdf:type rdf:resource="&owl;#FunctionalProperty" />
    <rdfs:domain rdf:resource="#CPUResource" />
    <rdfs:range rdf:resource="&units;#IntegerValue" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="branchPrediction">
    <rdf:type rdf:resource="&owl;#FunctionalProperty" />
    <rdfs:domain rdf:resource="#CPUResource" />
    <rdfs:range rdf:resource="&units;#StringValue" />
  </owl:ObjectProperty>

  <owl:Class rdf:ID="StorageResource">
    <rdfs:subClassOf rdf:resource="#Resource" />
  </owl:Class>



<!--   ############ ########### ############ ############ ########### -->
<!--   ############                                       ########### -->
<!--   ############          HARDWARE PARAMETERS          ########### -->
<!--   ############                                       ########### -->
<!--   ############ ########### ############ ############ ########### -->

  <owl:Class rdf:ID="Hardware" />

  <owl:Class rdf:ID="IODevice">
    <rdfs:subClassOf rdf:resource="#Hardware" />
  </owl:Class>

  <owl:Class rdf:ID="OutputDevice">
    <rdfs:subClassOf rdf:resource="#IODevice" />
  </owl:Class>

  <owl:Class rdf:ID="InputDevice">
    <rdfs:subClassOf rdf:resource="#IODevice" />
  </owl:Class>



<!--   ############ ########### ############ ############ ########### -->
<!--   ############                                       ########### -->
<!--   ############          SOFTWARE PARAMETERS          ########### -->
<!--   ############                                       ########### -->
<!--   ############ ########### ############ ############ ########### -->

  <owl:Class rdf:ID="Software" />  

  <owl:Class rdf:ID="VirtualMachine">
    <rdfs:subClassOf rdf:resource="#Software" />
  </owl:Class>

  <owl:Class rdf:ID="RenderingEngine">
    <rdfs:subClassOf rdf:resource="#Software" />
  </owl:Class>

  <owl:Class rdf:ID="Library">
    <rdfs:subClassOf rdf:resource="#Software" />
  </owl:Class>

  <owl:Class rdf:ID="OperatingSystem">
    <rdfs:subClassOf rdf:resource="#Software" />
  </owl:Class>

  <owl:Class rdf:ID="Middleware">
    <rdfs:subClassOf rdf:resource="#Software" />
  </owl:Class>

  <owl:Class rdf:ID="Version" />

  <owl:ObjectProperty rdf:ID="hasSubVersion">
    <rdf:type rdf:resource="&owl;#FunctionalProperty" />
    <rdfs:domain rdf:resource="#Version" />
    <rdfs:range rdf:resource="#Version" />
  </owl:ObjectProperty>

  <owl:DatatypeProperty rdf:ID="versionNumber">
    <rdf:type rdf:resource="&owl;#FunctionalProperty" />
    <rdfs:range rdf:resource="&xsd;#int" />
    <rdfs:domain rdf:resource="#Version" />
  </owl:DatatypeProperty>

  <owl:Class rdf:ID="Modality" />

  <owl:ObjectProperty rdf:ID="supportsModality">
    <rdfs:domain rdf:resource="#RenderingEngine" />
    <rdfs:range rdf:resource="#Modality" />
  </owl:ObjectProperty>



<!--   ############ ########### ############ ############ ########### -->
<!--   ############                                       ########### -->
<!--   ############          PLATFORM PARAMETERS          ########### -->
<!--   ############                                       ########### -->
<!--   ############ ########### ############ ############ ########### -->

  <owl:Class rdf:ID="Platform" />  

  <owl:ObjectProperty rdf:ID="providesService">
    <rdfs:range rdf:resource="#Service" />
    <rdfs:domain rdf:resource="#Software" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="providesSoftware">
    <rdfs:range rdf:resource="#Software" />
    <rdfs:domain rdf:resource="#Platform" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="providesHardware">
    <rdfs:domain rdf:resource="#Platform" />
    <rdfs:range rdf:resource="#Hardware" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="requiresPlatform">
    <rdfs:domain rdf:resource="#Software" />
    <rdfs:range rdf:resource="#Platform" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="hasEnvironment">
    <rdf:type rdf:resource="&owl;#FunctionalProperty" />
    <rdfs:domain rdf:resource="#Platform" />
    <rdfs:range rdf:resource="#Environment" />
  </owl:ObjectProperty>



<!--   ############ ########### ############ ############ ########### -->
<!--   ############                                       ########### -->
<!--   ############            USER PARAMETERS            ########### -->
<!--   ############                                       ########### -->
<!--   ############ ########### ############ ############ ########### -->

  <owl:Class rdf:ID="User" />

  <owl:Class rdf:ID="Activity" />

  <owl:Class rdf:ID="Role" />

  <owl:Class rdf:ID="Mood" />

  <owl:Class rdf:ID="Task" />

  <owl:ObjectProperty rdf:ID="usesService">
    <rdfs:range rdf:resource="#Service" />
    <rdfs:domain rdf:resource="#Task" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="usesIODevice">
    <rdfs:domain rdf:resource="#User" />
    <rdfs:range rdf:resource="#IODevice" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="hasRole">
    <rdfs:domain rdf:resource="#User" />
    <rdfs:range rdf:resource="#Role" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="hasTask">
    <rdfs:domain rdf:resource="#User" />
    <rdfs:range rdf:resource="#Task" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="hasActivity">
    <rdfs:domain rdf:resource="#Task" />
    <rdfs:range rdf:resource="#Activity" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="hasMood">
    <rdfs:range rdf:resource="#Mood" />
    <rdfs:domain rdf:resource="#User" />
    <rdf:type rdf:resource="&owl;#FunctionalProperty" />
  </owl:ObjectProperty>



<!--   ############ ########### ############ ############ ########### -->
<!--   ############                                       ########### -->
<!--   ############            USER PREFERENCES           ########### -->
<!--   ############                                       ########### -->
<!--   ############ ########### ############ ############ ########### -->

  <owl:Class rdf:ID="Profile" />

  <owl:Class rdf:ID="ProfileItem" />

  <owl:Class rdf:ID="PreferenceItem">
    <rdfs:subClassOf rdf:resource="#ProfileItem" />
  </owl:Class>

  <owl:ObjectProperty rdf:ID="prefProperty">
    <rdf:type rdf:resource="&owl;#FunctionalProperty" />
    <rdfs:domain rdf:resource="#PreferenceItem" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="prefValue">
    <rdf:type rdf:resource="&owl;#FunctionalProperty" />
    <rdfs:domain rdf:resource="#PreferenceItem" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="prefCondition">
    <rdfs:domain rdf:resource="#PreferenceItem" />
  </owl:ObjectProperty>



  <owl:ObjectProperty rdf:ID="hasProfileItem">
    <rdfs:domain rdf:resource="#Profile" />
    <rdfs:range rdf:resource="#ProfileItem" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="hasProfile">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#User" />
          <owl:Class rdf:about="#Role" />
          <owl:Class rdf:about="#Mood" />
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="#Profile" />
    <rdf:type rdf:resource="&owl;#FunctionalProperty" />
  </owl:ObjectProperty>



<!--   ############ ########### ############ ############ ########### -->
<!--   ############                                       ########### -->
<!--   ############         ENVIRONMENT PARAMETERS        ########### -->
<!--   ############                                       ########### -->
<!--   ############ ########### ############ ############ ########### -->

  <owl:Class rdf:ID="Location" />

  <owl:Class rdf:ID="Coordinate">
    <rdfs:subClassOf rdf:resource="#Location" />
  </owl:Class>

  <owl:Class rdf:ID="Address">
    <rdfs:subClassOf rdf:resource="#Location" />
  </owl:Class>

  <owl:ObjectProperty rdf:ID="hasLocation">
    <rdfs:domain rdf:resource="#Environment" />
    <rdfs:range rdf:resource="#Location" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="isRelativeTo">
    <rdfs:range rdf:resource="#Location" />
  </owl:ObjectProperty>


  <owl:Class rdf:ID="Time" />

  <owl:ObjectProperty rdf:ID="hasTime">
    <rdfs:domain rdf:resource="#Environment" />
    <rdfs:range rdf:resource="#Time" />
  </owl:ObjectProperty>



  <owl:Class rdf:ID="Environment" />

  <owl:Class rdf:ID="EnvironmentalCondition" />
    

  <owl:Class rdf:ID="Pressure">
    <rdfs:subClassOf rdf:resource="#EnvironmentalCondition" />
  </owl:Class>

  <owl:Class rdf:ID="Humidity">
    <rdfs:subClassOf rdf:resource="#EnvironmentalCondition" />
  </owl:Class>

  <owl:Class rdf:ID="Noise">
    <rdfs:subClassOf rdf:resource="#EnvironmentalCondition" />
  </owl:Class>

  <owl:Class rdf:ID="Temperature">
    <rdfs:subClassOf rdf:resource="#EnvironmentalCondition" />
  </owl:Class>

  <owl:Class rdf:ID="Lighting">
    <rdfs:subClassOf rdf:resource="#EnvironmentalCondition" />
  </owl:Class>

  <owl:ObjectProperty rdf:ID="hasEnvironmentalCondition">
    <rdfs:domain rdf:resource="#Environment" />
    <rdfs:range rdf:resource="#EnvironmentalCondition" />
  </owl:ObjectProperty>

  
<!--   ############ ########### ############ ############ ########### -->
<!--   ############                                       ########### -->
<!--   ############                UNITS                  ########### -->
<!--   ############                                       ########### -->
<!--   ############ ########### ############ ############ ########### -->
  
  <owl:ObjectProperty rdf:ID="hasValue">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#EnvironmentCondition" />
          <owl:Class rdf:about="#Resource" />
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="&units;#Value" />
  </owl:ObjectProperty>
  
</rdf:RDF>
