<?xml version='1.0' encoding='ISO-8859-1'?>
<!DOCTYPE uridef[
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema">
  <!ENTITY owl "http://www.w3.org/2002/07/owl">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema">
  <!ENTITY context "http://www.cs.kuleuven.be/~distrinet/projects/CoDAMoS/2005/01/Context.owl">
  <!ENTITY java "http://ssel.vub.ac.be/codamos/2005/04/java.owl">
  <!ENTITY component "http://www.cs.kuleuven.be/~davy/owl/Component.owl">
  <!ENTITY DEFAULT "http://www.cs.kuleuven.be/~davy/owl/Draco.owl">
]>

<rdf:RDF
  xmlns:rdf = "&rdf;#"
  xmlns:rdfs = "&rdfs;#"
  xmlns:owl = "&owl;#"
  xmlns:xsd = "&xsd;#"
  xmlns:component = "&component;#" 
  xmlns:java = "&java;#" 
  xmlns:context = "&context;#"
  xmlns = "&DEFAULT;#"
  xml:base = "&DEFAULT;">
 
  <owl:Ontology rdf:about="">
    <owl:versionInfo>
      $Id: Draco.owl,v 1.0 2005/07/10 19:52 davy Exp $
    </owl:versionInfo> 
    <rdfs:comment>
      Draco ontology
      Created by:
        Davy Preuveneers
    </rdfs:comment> 
    <owl:imports rdf:resource="&context;" />
    <owl:imports rdf:resource="&java;" />
    <owl:imports rdf:resource="&component;" />
  </owl:Ontology>

  <owl:Class rdf:ID="Draco">
    <rdfs:subClassOf rdf:resource="&context;#Middleware" />
  </owl:Class>

  <owl:ObjectProperty rdf:ID="hasVersion">
    <rdf:type rdf:resource="&owl;#FunctionalProperty" />
    <rdfs:domain rdf:resource="#Draco" />
    <rdfs:range rdf:resource="&context;#Version" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="hasComponentComposition">
    <rdfs:domain rdf:resource="#Draco" />
    <rdfs:range rdf:resource="&component;#ComponentComposition" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="hasJavaVM">
    <rdfs:domain rdf:resource="#Draco" />
    <rdfs:range rdf:resource="&java;#JavaVM" />
    <rdf:type rdf:resource="&owl;#FunctionalProperty" />
  </owl:ObjectProperty>


  <context:Version rdf:ID="version2.0">
    <context:versionNumber rdf:datatype="&xsd;#int">2</context:versionNumber>
    <context:hasSubVersion>
      <context:Version rdf:ID="subversion0">
        <context:versionNumber rdf:datatype="&xsd;#int">0</context:versionNumber>
      </context:Version>
    </context:hasSubVersion>
  </context:Version>

  <context:Version rdf:ID="version2.1">
    <context:versionNumber rdf:datatype="&xsd;#int">2</context:versionNumber>
    <context:hasSubVersion>
      <context:Version rdf:ID="subversion1">
        <context:versionNumber rdf:datatype="&xsd;#int">1</context:versionNumber>
      </context:Version>
    </context:hasSubVersion>
  </context:Version>
 
</rdf:RDF>

