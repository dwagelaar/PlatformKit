<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns="http://ssel.vub.ac.be/codamos/2005/03/feature.owl#"
  xml:base="http://ssel.vub.ac.be/codamos/2005/03/feature.owl">
  <owl:Ontology rdf:about="">
    <rdfs:label xml:lang="en">SSEL Feature Model</rdfs:label>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >SSEL-2005/03</owl:versionInfo>
    <rdfs:comment xml:lang="en">OWL framework for expressing Feature Models</rdfs:comment>
  </owl:Ontology>
  <owl:Class rdf:ID="Feature"/>
  <owl:Class rdf:ID="ImpliesInvalidConfiguration">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="InvalidConfiguration"/>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="transitivelyImpliesFeature"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#InvalidConfiguration"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="ValidConfiguration">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Configuration"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Configuration">
    <rdfs:subClassOf rdf:resource="#Feature"/>
  </owl:Class>
  <owl:Class rdf:about="#InvalidConfiguration">
    <rdfs:subClassOf rdf:resource="#Configuration"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="impliesFeature">
    <rdfs:range rdf:resource="#Feature"/>
    <rdfs:domain rdf:resource="#Feature"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Every feature must at least imply itself</rdfs:comment>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#transitivelyImpliesFeature"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="requires">
    <rdfs:domain rdf:resource="#Feature"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#transitivelyImpliesFeature">
    <rdfs:domain rdf:resource="#Feature"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    <rdfs:range rdf:resource="#Feature"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="implementation">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain rdf:resource="#Feature"/>
  </owl:DatatypeProperty>
</rdf:RDF>

