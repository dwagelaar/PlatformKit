                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_n_-3cC2BEd2e7od4K9wNBA" name="DiscoveryApplet_startBehavior" specification="_RYXKUC2BEd2e7od4K9wNBA">
                  <language>java</language>
                  <body>&#x9;&#x9;getView().setStatus(&quot;Discovering platform...&quot;);&#xA;&#x9;&#x9;final PlatformOntology ont = new PlatformOntology();&#xA;&#x9;&#x9;setOntology(ont);&#xA;&#xA;&#x9;&#x9;final String javaVendor = System.getProperty(&quot;java.vendor&quot;, &quot;unknown&quot;);&#xA;&#x9;&#x9;final String javaVersion = System&#xA;&#x9;&#x9;&#x9;&#x9;.getProperty(&quot;java.version&quot;, &quot;unknown&quot;);&#xA;&#x9;&#x9;final String javaSpecName = System.getProperty(&#xA;&#x9;&#x9;&#x9;&#x9;&quot;java.specification.name&quot;, &quot;unknown&quot;);&#xA;&#x9;&#x9;final String javaSpecVersion = System.getProperty(&#xA;&#x9;&#x9;&#x9;&#x9;&quot;java.specification.version&quot;, &quot;unknown&quot;);&#xA;&#x9;&#x9;final String javaVmName = System.getProperty(&quot;java.vm.name&quot;, &quot;unknown&quot;);&#xA;&#x9;&#x9;final String javaVmVersion = System.getProperty(&quot;java.vm.version&quot;,&#xA;&#x9;&#x9;&#x9;&#x9;&quot;unknown&quot;);&#xA;&#x9;&#x9;final String javaVmSpecName = System.getProperty(&#xA;&#x9;&#x9;&#x9;&#x9;&quot;java.vm.specification.name&quot;, &quot;unknown&quot;);&#xA;&#x9;&#x9;final String javaVmSpecVersion = System.getProperty(&#xA;&#x9;&#x9;&#x9;&#x9;&quot;java.vm.specification.version&quot;, &quot;unknown&quot;);&#xA;&#x9;&#x9;final String osName = System.getProperty(&quot;os.name&quot;, &quot;unknown&quot;);&#xA;&#x9;&#x9;final String osArch = System.getProperty(&quot;os.arch&quot;, &quot;unknown&quot;);&#xA;&#x9;&#x9;final String osVersion = System.getProperty(&quot;os.version&quot;, &quot;unknown&quot;);&#xA;&#xA;&#x9;&#x9;// Platform&#xA;&#x9;&#x9;final Individual platform = new Individual();&#xA;&#x9;&#x9;platform.setClassNS(&quot;platform&quot;);&#xA;&#x9;&#x9;platform.setClassName(&quot;Platform&quot;);&#xA;&#x9;&#x9;platform.setName(&quot;DynamicallyDiscoveredPlatform&quot;);&#xA;&#x9;&#x9;ont.addIndividual(platform);&#xA;&#x9;&#x9;// JRE&#xA;&#x9;&#x9;final Individual jre = new Individual();&#xA;&#x9;&#x9;jre.setClassNS(&quot;java&quot;);&#xA;&#x9;&#x9;jre.setClassName(&quot;JRE&quot;);&#xA;&#x9;&#x9;final StringBuffer jreName = new StringBuffer();&#xA;&#x9;&#x9;jreName.append(javaVendor.replace(' ', '-'));&#xA;&#x9;&#x9;jreName.append(&quot;-JRE-&quot;);&#xA;&#x9;&#x9;jreName.append(javaVersion.replace(' ', '-'));&#xA;&#x9;&#x9;jre.setName(jreName.toString());&#xA;&#x9;&#x9;ont.addIndividual(jre);&#xA;&#x9;&#x9;// Platform providesSoftware JRE&#xA;&#x9;&#x9;final Slot prJre = new Slot();&#xA;&#x9;&#x9;prJre.setIndividual(platform);&#xA;&#x9;&#x9;prJre.setClassNS(&quot;platform&quot;);&#xA;&#x9;&#x9;prJre.setClassName(&quot;providesSoftware&quot;);&#xA;&#x9;&#x9;prJre.setValue(jre);&#xA;&#x9;&#x9;// ClassLibrary&#xA;&#x9;&#x9;final Individual clsLib = new Individual();&#xA;&#x9;&#x9;clsLib.setClassNS(&quot;java&quot;);&#xA;&#x9;&#x9;clsLib.setClassName(&quot;JavaLibrary&quot;);&#xA;&#x9;&#x9;clsLib.setName(jre.getName() + &quot;-ClassLibrary&quot;);&#xA;&#x9;&#x9;ont.addIndividual(clsLib);&#xA;&#x9;&#x9;// JRE providesBuiltinClassLibrary ClassLibrary&#xA;&#x9;&#x9;final Slot prClsLib = new Slot();&#xA;&#x9;&#x9;prClsLib.setIndividual(jre);&#xA;&#x9;&#x9;prClsLib.setClassNS(&quot;java&quot;);&#xA;&#x9;&#x9;prClsLib.setClassName(&quot;providesBuiltinJavaLibrary&quot;);&#xA;&#x9;&#x9;prClsLib.setValue(clsLib);&#xA;&#x9;&#x9;// JavaVM&#xA;&#x9;&#x9;final Individual jvm = new Individual();&#xA;&#x9;&#x9;jvm.setClassNS(&quot;java&quot;);&#xA;&#x9;&#x9;jvm.setClassName(&quot;JavaVM&quot;);&#xA;&#x9;&#x9;final StringBuffer jvmName = new StringBuffer();&#xA;&#x9;&#x9;jvmName.append(javaVmName.replace(' ', '-'));&#xA;&#x9;&#x9;jvmName.append(&quot;-&quot;);&#xA;&#x9;&#x9;jvmName.append(javaVmVersion.replace(' ', '-'));&#xA;&#x9;&#x9;jvm.setName(jvmName.toString());&#xA;&#x9;&#x9;ont.addIndividual(jvm);&#xA;&#x9;&#x9;// JRE providesJavaVM JavaVM&#xA;&#x9;&#x9;final Slot prJVM = new Slot();&#xA;&#x9;&#x9;prJVM.setIndividual(jre);&#xA;&#x9;&#x9;prJVM.setClassNS(&quot;java&quot;);&#xA;&#x9;&#x9;prJVM.setClassName(&quot;providesJavaVM&quot;);&#xA;&#x9;&#x9;prJVM.setValue(jvm);&#xA;&#x9;&#x9;// JavaBytecodeFormat&#xA;&#x9;&#x9;final Individual bcformat = new Individual();&#xA;&#x9;&#x9;bcformat.setNamespace(&quot;java&quot;);&#xA;&#x9;&#x9;bcformat.setName(&quot;Java1.1BytecodeFormat&quot;);&#xA;&#x9;&#x9;// JavaVM supportsBytecodeFormat JavaBytecodeFormat&#xA;&#x9;&#x9;final Slot supBCF = new Slot();&#xA;&#x9;&#x9;supBCF.setIndividual(jvm);&#xA;&#x9;&#x9;supBCF.setClassNS(&quot;java&quot;);&#xA;&#x9;&#x9;supBCF.setClassName(&quot;supportsBytecodeFormat&quot;);&#xA;&#x9;&#x9;supBCF.setValue(bcformat);&#xA;&#x9;&#x9;// OperatingSystem&#xA;&#x9;&#x9;final Individual os = new Individual();&#xA;&#x9;&#x9;os.setClassNS(&quot;platform&quot;);&#xA;&#x9;&#x9;os.setClassName(&quot;OperatingSystem&quot;);&#xA;&#x9;&#x9;final StringBuffer osId = new StringBuffer();&#xA;&#x9;&#x9;osId.append(osName.replace(' ', '-'));&#xA;&#x9;&#x9;osId.append(&quot;-&quot;);&#xA;&#x9;&#x9;osId.append(osArch.replace(' ', '-'));&#xA;&#x9;&#x9;osId.append(&quot;-&quot;);&#xA;&#x9;&#x9;osId.append(osVersion.replace(' ', '-'));&#xA;&#x9;&#x9;os.setName(osId.toString());&#xA;&#x9;&#x9;ont.addIndividual(os);&#xA;&#x9;&#x9;// Platform providesSoftware OperatingSystem&#xA;&#x9;&#x9;final Slot prOS = new Slot();&#xA;&#x9;&#x9;prOS.setIndividual(platform);&#xA;&#x9;&#x9;prOS.setClassNS(&quot;platform&quot;);&#xA;&#x9;&#x9;prOS.setClassName(&quot;providesSoftware&quot;);&#xA;&#x9;&#x9;prOS.setValue(os);&#xA;&#x9;&#x9;// CPUResource&#xA;&#x9;&#x9;final Individual cpu = new Individual();&#xA;&#x9;&#x9;cpu.setClassNS(&quot;platform&quot;);&#xA;&#x9;&#x9;cpu.setClassName(&quot;CPUResource&quot;);&#xA;&#x9;&#x9;cpu.setName(&quot;SomeKindOf&quot; + osArch);&#xA;&#x9;&#x9;ont.addIndividual(cpu);&#xA;&#x9;&#x9;// Platform providesHardware CPUResource&#xA;&#x9;&#x9;final Slot prCPU = new Slot();&#xA;&#x9;&#x9;prCPU.setIndividual(platform);&#xA;&#x9;&#x9;prCPU.setClassNS(&quot;platform&quot;);&#xA;&#x9;&#x9;prCPU.setClassName(&quot;providesHardware&quot;);&#xA;&#x9;&#x9;prCPU.setValue(cpu);&#xA;&#x9;&#x9;// StringValue (*&amp;^%^$%^$ StringValue.... XSD wasn't good enough?!)&#xA;&#x9;&#x9;final Individual instr = new Individual();&#xA;&#x9;&#x9;instr.setClassNS(&quot;units&quot;);&#xA;&#x9;&#x9;instr.setClassName(&quot;StringValue&quot;);&#xA;&#x9;&#x9;instr.setName(osArch);&#xA;&#x9;&#x9;ont.addIndividual(instr);&#xA;&#x9;&#x9;// CPUResource instructionSet StringValue&#xA;&#x9;&#x9;final Slot instrSet = new Slot();&#xA;&#x9;&#x9;instrSet.setIndividual(cpu);&#xA;&#x9;&#x9;instrSet.setClassNS(&quot;platform&quot;);&#xA;&#x9;&#x9;instrSet.setClassName(&quot;instructionSet&quot;);&#xA;&#x9;&#x9;instrSet.setValue(instr);&#xA;&#x9;&#x9;// String&#xA;&#x9;&#x9;final Individual instrStr = new Individual();&#xA;&#x9;&#x9;instrStr.setClassNS(&quot;xsd&quot;);&#xA;&#x9;&#x9;instrStr.setClassName(&quot;string&quot;);&#xA;&#x9;&#x9;instrStr.setName(osArch);&#xA;&#x9;&#x9;// StringValue sval String&#xA;&#x9;&#x9;final Slot svalInstr = new Slot();&#xA;&#x9;&#x9;svalInstr.setIndividual(instr);&#xA;&#x9;&#x9;svalInstr.setClassNS(&quot;units&quot;);&#xA;&#x9;&#x9;svalInstr.setClassName(&quot;sval&quot;);&#xA;&#x9;&#x9;svalInstr.setValue(instrStr);&#xA;&#xA;&#x9;&#x9;if (&quot;Java Platform API Specification&quot;.equals(javaSpecName)) { // JDK&#xA;&#x9;&#x9;&#x9;// JavaWebApplet&#xA;&#x9;&#x9;&#x9;final Individual jwa = new Individual();&#xA;&#x9;&#x9;&#x9;jwa.setClassNS(&quot;java&quot;);&#xA;&#x9;&#x9;&#x9;jwa.setClassName(&quot;JavaWebApplet&quot;);&#xA;&#x9;&#x9;&#x9;jwa.setName(jre.getName() + &quot;-JavaWebApplet&quot;);&#xA;&#x9;&#x9;&#x9;ont.addIndividual(jwa);&#xA;&#x9;&#x9;&#x9;// JRE providesJavaPackageManager JavaWebApplet&#xA;&#x9;&#x9;&#x9;final Slot prJWA = new Slot();&#xA;&#x9;&#x9;&#x9;prJWA.setIndividual(jre);&#xA;&#x9;&#x9;&#x9;prJWA.setClassNS(&quot;java&quot;);&#xA;&#x9;&#x9;&#x9;prJWA.setClassName(&quot;providesJavaPackageManager&quot;);&#xA;&#x9;&#x9;&#x9;prJWA.setValue(jwa);&#xA;&#x9;&#x9;&#x9;// JavaWebStart&#xA;&#x9;&#x9;&#x9;final Individual jws = new Individual();&#xA;&#x9;&#x9;&#x9;jws.setClassNS(&quot;java&quot;);&#xA;&#x9;&#x9;&#x9;jws.setClassName(&quot;JavaWebStart&quot;);&#xA;&#x9;&#x9;&#x9;jws.setName(jre.getName() + &quot;-JavaWebStart&quot;);&#xA;&#x9;&#x9;&#x9;// JRE providesJavaPackageManager JavaWebStart&#xA;&#x9;&#x9;&#x9;final Slot prJWS = new Slot();&#xA;&#x9;&#x9;&#x9;prJWS.setClassNS(&quot;java&quot;);&#xA;&#x9;&#x9;&#x9;prJWS.setClassName(&quot;providesJavaPackageManager&quot;);&#xA;&#x9;&#x9;&#x9;prJWS.setValue(jws);&#xA;&#x9;&#x9;&#x9;// parse version number&#xA;&#x9;&#x9;&#x9;try {&#xA;&#x9;&#x9;&#x9;&#x9;final int jv = Integer.parseInt(javaSpecVersion.replace('.',&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;'0'));&#xA;&#x9;&#x9;&#x9;&#x9;if (jv >= 101) {&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;switch (jv) {&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;case 101 :&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;jre.setClassNS(&quot;jdk-1_1&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;jre.setClassName(&quot;Jdk-1_1&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;bcformat.setName(&quot;Java1.1BytecodeFormat&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;break;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;case 102 :&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;jre.setClassNS(&quot;j2se-1_2&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;jre.setClassName(&quot;J2se-1_2&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;bcformat.setName(&quot;Java1.2BytecodeFormat&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;break;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;case 103 :&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;jre.setClassNS(&quot;j2se-1_3&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;jre.setClassName(&quot;J2se-1_3&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;bcformat.setName(&quot;Java1.3BytecodeFormat&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;break;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;case 104 :&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;jre.setClassNS(&quot;j2se-1_4&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;jre.setClassName(&quot;J2se-1_4&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;bcformat.setName(&quot;Java1.4BytecodeFormat&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ont.addIndividual(jws);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;prJWS.setIndividual(jre);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;break;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;case 105 :&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;jre.setClassNS(&quot;j2se-1_5&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;jre.setClassName(&quot;J2se-1_5&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;bcformat.setName(&quot;Java5BytecodeFormat&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ont.addIndividual(jws);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;prJWS.setIndividual(jre);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;break;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;default : // 1.6 and up&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;jre.setClassNS(&quot;j2se-1_6&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;jre.setClassName(&quot;J2se-1_6&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;bcformat.setName(&quot;Java6BytecodeFormat&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ont.addIndividual(jws);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;prJWS.setIndividual(jre);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;break;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;}&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;clsLib.setClassNS(jre.getClassNS());&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;clsLib.setClassName(jre.getClassName() + &quot;ClassLibrary&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;}&#xA;&#x9;&#x9;&#x9;} catch (NumberFormatException e) {&#xA;&#x9;&#x9;&#x9;}&#xA;&#x9;&#x9;} else if (javaSpecName.startsWith(&quot;J2ME Personal Profile&quot;)) {&#xA;&#x9;&#x9;&#x9;try {&#xA;&#x9;&#x9;&#x9;&#x9;final int jv = Integer.parseInt(javaSpecVersion.replace('.',&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;'0'));&#xA;&#x9;&#x9;&#x9;&#x9;switch (jv) {&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;case 100 :&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;jre.setClassNS(&quot;j2me-pp-1_0&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;jre.setClassName(&quot;J2me-pp-1_0&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;bcformat.setName(&quot;Java1.3BytecodeFormat&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;break;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;default : // 1.1 and up&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;jre.setClassNS(&quot;j2me-pp-1_1&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;jre.setClassName(&quot;J2me-pp-1_1&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;bcformat.setName(&quot;Java1.4BytecodeFormat&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;break;&#xA;&#x9;&#x9;&#x9;&#x9;}&#xA;&#x9;&#x9;&#x9;&#x9;clsLib.setClassNS(jre.getClassNS());&#xA;&#x9;&#x9;&#x9;&#x9;clsLib.setClassName(jre.getClassName() + &quot;ClassLibrary&quot;);&#xA;&#x9;&#x9;&#x9;} catch (NumberFormatException e) {&#xA;&#x9;&#x9;&#x9;}&#xA;&#x9;&#x9;} else if (javaVendor.equals(&quot;Insignia Solutions Inc.&quot;)) { // Jeode&#xA;&#x9;&#x9;&#x9;try {&#xA;&#x9;&#x9;&#x9;&#x9;final int jv = Integer.parseInt(javaVersion.replace('.', '0'));&#xA;&#x9;&#x9;&#x9;&#x9;if (jv >= 301) { // Jeode 3.1 == PersonalJava 1.2&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;jre.setClassNS(&quot;personaljava-1_1&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;jre.setClassName(&quot;Personaljava-1_1&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;bcformat.setName(&quot;Java1.1BytecodeFormat&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;}&#xA;&#x9;&#x9;&#x9;&#x9;clsLib.setClassNS(jre.getClassNS());&#xA;&#x9;&#x9;&#x9;&#x9;clsLib.setClassName(jre.getClassName() + &quot;ClassLibrary&quot;);&#xA;&#x9;&#x9;&#x9;} catch (NumberFormatException e) {&#xA;&#x9;&#x9;&#x9;}&#xA;&#x9;&#x9;} else {&#xA;&#x9;&#x9;&#x9;System.out.println(&quot;java.vendor = &quot; + javaVendor);&#xA;&#x9;&#x9;&#x9;System.out.println(&quot;java.version = &quot; + javaVersion);&#xA;&#x9;&#x9;&#x9;System.out.println(&quot;java.specification.name = &quot; + javaSpecName);&#xA;&#x9;&#x9;&#x9;System.out.println(&quot;java.specification.version = &quot;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;+ javaSpecVersion);&#xA;&#x9;&#x9;&#x9;System.out.println(&quot;java.vm.name = &quot; + javaVmName);&#xA;&#x9;&#x9;&#x9;System.out.println(&quot;java.vm.version = &quot; + javaVmVersion);&#xA;&#x9;&#x9;&#x9;System.out&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;.println(&quot;java.vm.specification.name = &quot; + javaVmSpecName);&#xA;&#x9;&#x9;&#x9;System.out.println(&quot;java.vm.specification.version = &quot;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;+ javaVmSpecVersion);&#xA;&#x9;&#x9;}&#xA;&#xA;&#x9;&#x9;getView().setStatus(&quot;Platform discovered&quot;);&#xA;&#x9;&#x9;getView().setEnabled(true);</body>
